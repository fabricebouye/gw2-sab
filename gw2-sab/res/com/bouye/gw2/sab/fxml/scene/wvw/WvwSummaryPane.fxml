<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>


<BorderPane id="BorderPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.65" fx:controller="com.bouye.gw2.sab.scene.wvw.WvwSummaryPaneController">
    <center>
        <PieChart fx:id="mainPieChart" BorderPane.alignment="CENTER" />
    </center>
    <right>
        <VBox prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
                <PieChart fx:id="ebPieChart" />
                <PieChart fx:id="redPieChart" />
                <PieChart fx:id="bluePieChart" />
                <PieChart fx:id="greenPieChart" />
            </children>
        </VBox>
    </right>
</BorderPane>
