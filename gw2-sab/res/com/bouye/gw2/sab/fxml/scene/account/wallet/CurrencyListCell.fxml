<?import javafx.scene.control.ListCell?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root type="ListCell" id="ListCell" styleClass="currency-list-cell" fx:id="rootPane" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bouye.gw2.sab.scene.account.wallet.CurrencyListCellController">
    <graphic>
        <HBox id="HBox" prefWidth="200.0" >
            <children>
                <Label fx:id="nameLabel" minWidth="50"  maxWidth="1.7976931348623157E308" text="Label" HBox.hgrow="ALWAYS" />
                <Label fx:id="amountLabel" text="Label" />
                <Label fx:id="iconLabel">
                    <graphic>
                        <ImageView fx:id="icon" fitWidth="32.0" preserveRatio="true" />
                    </graphic>
                    <tooltip>
                        <Tooltip fx:id="infoTip" maxWidth="350.0" text="Label" wrapText="true" />
                    </tooltip>
                </Label>
            </children>
        </HBox>
    </graphic>
</fx:root>
