    <children>
        <Label fx:id="nameLabel" text="Label" />
        <Label fx:id="regionLabel" text="Label" GridPane.columnIndex="2" />
        <Label fx:id="languageLabel" text="Label" GridPane.columnIndex="3" />
        <Label fx:id="populationLabel" text="Label" GridPane.columnSpan="2147483647" GridPane.rowIndex="1" />
    </children>
    <columnConstraints>
        <ColumnConstraints hgrow="NEVER" />
        <ColumnConstraints fillWidth="true" hgrow="ALWAYS" minWidth="75.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="NEVER" />
        <ColumnConstraints hgrow="NEVER" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints vgrow="NEVER" />
        <RowConstraints vgrow="NEVER" />
        <RowConstraints vgrow="NEVER" />
        <RowConstraints vgrow="ALWAYS" />
    </rowConstraints>

</GridPane>
