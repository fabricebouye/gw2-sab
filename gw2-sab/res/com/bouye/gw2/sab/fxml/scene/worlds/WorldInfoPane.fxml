<fx:root fx:id="rootPane" prefHeight="200.0" prefWidth="600.0" styleClass="world-info-pane" type="GridPane" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bouye.gw2.sab.scene.worlds.WorldInfoPaneController">
    <children>
        <Label fx:id="nameLabel" text="Label" styleClass="strong-label" />
        <Label fx:id="regionLabel" text="Label" GridPane.columnIndex="2" />
        <Label fx:id="languageLabel" text="Label" GridPane.columnIndex="3" />
